<script language="javascript">
    function changeImage() {

        if (document.getElementById("imgClickAndChange").src == "https://img.purch.com/w/660/aHR0cDovL3d3dy5saXZlc2NpZW5jZS5jb20vaW1hZ2VzL2kvMDAwLzEwNC84MDAvb3JpZ2luYWwvc2h1dHRlcnN0b2NrXzcyMjcxODA3OS5qcGc=") 
        {
            document.getElementById("imgClickAndChange").src = "https://files.guidedanmark.org/files/483/164930_Street-Food.jpg";
        }
        else 
        {
            document.getElementById("imgClickAndChange").src = "https://caspiannews.com/media/caspian_news/all_original_photos/1528831479_7183783_1528831390_5761793SFF-Foto-2018-001web2.jpg";
        }
    }
</script>
